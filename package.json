{
  "name": "r6maps",
  "//": "When changing version # search and replace in this file and index.html.  This is hacky for now but it avoided some complexities.",
  "version": "1.2.9",
  "description": "Rainbow Six Siege map quick references",
  "watch": {
    "build:js": "dev/js/*.js"
  },
  "scripts": {
    "test:js": "eslint dev/js/*.js dev/utils/*.js",
    "test": "npm run test:js",
    "mkdir:css": "mkdirp ./site/css",
    "build:aboutcss": "npm run mkdir:css && scss './dev/scss/about.scss':'./site/css/about.1.1.0.css'",
    "build:css": "npm run mkdir:css && scss './dev/scss/main.scss':'./site/css/main.1.2.9.css'",
    "watch:css": "npm run mkdir:css && scss --watch './dev/scss/main.scss':'./site/css/main.1.2.9.css'",
    "mkdir:js": "mkdirp ./site/js",
    "build:js": "npm run mkdir:js && uglifyjs --compress --mangle -o ./site/js/r6-maps.1.2.9.min.js --source-map  ./site/js/r6-maps.1.2.9.min.js.map --source-map-url r6-maps.1.2.9.min.js.map --source-map-include-sources -- ./dev/js/langs/r6-maps.lang-terms.js ./dev/js/langs/r6-maps.lang-terms.*.js ./dev/js/r6-maps.map-data.js ./dev/js/r6-maps.map-render.js ./dev/js/r6-maps.controls.js ./dev/js/r6-maps.select-maps.js ./dev/js/r6-maps.drawing.js ./dev/js/r6-maps.sessions.js ./dev/js/r6-maps.main.js",
    "watch:js": "npm run mkdir:js && npm-watch",
    "build": "npm run build:css && npm run build:aboutcss && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capajon/r6maps.git"
  },
  "keywords": [
    "Rainbow",
    "Six",
    "Maps",
    "Siege"
  ],
  "author": "Jon Capa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capajon/r6maps/issues"
  },
  "homepage": "https://github.com/capajon/r6maps#readme",
  "devDependencies": {
    "eslint": "^2.2.0",
    "mkdirp": "^0.5.1",
    "npm-watch": "^0.1.7",
    "rimraf": "^2.5.4"
  }
}
